{
  "name": "@heiwa4126/hello-ts",
  "version": "1.0.5",
  "description": "An example of npm module in Typescript",
  "scripts": {
    "webpack": "webpack",
    "build": "tsc",
    "build:mjs": "tsc --project ./tsconfig.mjs.json",
    "build:all": "tsc && tsc --project ./tsconfig.mjs.json && webpack && rm dist/mjs/*.js",
    "watch": "tsc -w",
    "test": "jest",
    "start": "node dist/src/hello.js",
    "example:hello": "node dist/examples/hello.js"
  },
  "keywords": [],
  "author": "heiwa4126@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.4.2",
    "jest": "^29.5.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heiwa4126/npm-hello-ts"
  },
  "exports": {
    "import": "./dist/mjs/hello.mjs",
    "require": "./dist/src/hello.js",
    "node": "./dist/mjs/hello.mjs",
    "default": "./dist/src/hello.js"
  },
  "types": "./dist/mjs/hello.d.ts",
  "files": [
    "dist/src",
    "dist/mjs",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@heiwa4126/hello1": "^1.0.2"
  },
  "peerDependencies": {
    "@heiwa4126/hello1": "^1"
  }
}
